<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wxq.shopinterface.mapper.OrderMapper">

    <resultMap id="baseMap" type="com.wxq.bean.Order">
        <id column="id" property="id"/>
        <result column="order_code" property="orderCode"/>
        <result column="account" property="account"/>
        <result column="order_status" property="orderStatus"/>
        <result column="pay_status" property="payStatus"/>
        <result column="order_amount" property="orderAmount"/>
        <result column="create_time" property="createTime"/>
        <result column="create_by" property="createBy"/>
        <collection property="orderDetailList" column="order_code" javaType="com.wxq.bean.OrderDetail"
                    select="com.wxq.shopinterface.mapper.OrderDetailMapper.findList"/>
    </resultMap>

</mapper>

<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wxq.shopinterface.mapper.AttrValueMapper">

    <resultMap id="baseMap" type="com.wxq.bean.AttrValue">
        <id column="id" property="id"/>
        <result column="name" property="name"/>
        <result column="attr_id" property="attrId"/>
        <result column="is_delete" property="isDelete"/>
    </resultMap>

    <select id="findList" parameterType="int" resultMap="baseMap">
        /*select id,name,attr_id,is_delete from attr_value where id=#{id}*/
    </select>

    <select id="findById" parameterType="int" resultMap="baseMap">
        select id,name,attr_id,is_delete from attr_value where id=#{id}
    </select>
</mapper>
